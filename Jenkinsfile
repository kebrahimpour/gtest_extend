         stage ("prebuild"){
            steps {
                echo 'prebuild'

            }   

        }
        stage ("build"){
            steps {
                echo 'build'

            }   

        }

         stage ("postbuild"){
            steps {
                echo 'postbuild'

            }   

        }

        stage ("test"){
            steps {

                echo 'test'

            }   

        }

        stage ("deploy"){
            steps {
                echo 'deploy'

            }   

        }
         stage ("postdeploy"){
            steps {
                echo 'postdeploy'

            }   

        }
    }  
}
